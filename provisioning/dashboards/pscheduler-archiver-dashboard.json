          "rawQuery": true,
          "rawSql": "SELECT\n\t$__time(r.ts),\n\tr.src AS \"Source\",\n\tr.dst AS \"Destination\",\n\tr.hop_count AS \"Hop Count\",\n\tr.route_str AS \"Route (IP sequence)\"\nFROM (\n\tSELECT\n\t\tMIN(ts) AS ts,\n\t\trun_id,\n\t\tsrc,\n\t\tdst,\n\t\tSTRING_AGG(hop_ip, ' → ' ORDER BY hop_idx) AS route_str,\n\t\tCOUNT(*) AS hop_count\n\tFROM ps_trace_hops\n\tWHERE $__timeFilter(ts)\n\t\tAND (COALESCE('${src}','')='' OR src='${src}')\n\t\tAND (COALESCE('${dst}','')='' OR dst='${dst}')\n\tGROUP BY run_id, src, dst\n) r\nORDER BY time DESC\nLIMIT 500;",
